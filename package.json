{
  "name": "otter-editor",
  "description": "Content editor with declaratively defined content blocks.",
  "version": "0.0.50",
  "author": "Ben Hallstein <ben@ben.am> (https://ben.am/)",
  "license": "SEE LICENSE IN LICENSE.md",
  "homepage": "https://github.com/bhallstein/otter",
  "repository": {
    "type": "git",
    "url": "https://github.com/bhallstein/otter.git"
  },
  "files": [
    "README.md",
    "LICENSE.md",
    "dist",
    "src/index.css",
    "src/quill.snow.css"
  ],
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist && rm -rf demo/dist",
    "build": "npm run clean && webpack --env prod",
    "build--npm": "npm run clean && npm run build && mkdir -p dist && cp -R demo/dist/*.css dist && babel src/index.js -d dist && babel src/core -d dist/core",
    "prepublishOnly": "npm run build--npm",
    "dev": "webpack-dev-server --mode development",
    "dev--plugin": "babel -w src/index.js -d otter-plugin/dev/deps/otter-editor & babel -w src/core -d otter-plugin/dev/deps/otter-editor/core",
    "t": "ava -c 1 --verbose src/test/*",
    "tw": "ava -c 1 --watch src/test/*",
    "ts": "ava -c 1 --watch",
    "i": "rm -rf node_modules && npm install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "bulma": "^0.9.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-quill": "^1.3.5",
    "react-toggle": "^4.1.1"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.11.5",
    "ava": "^3.13.0",
    "babel-loader": "^8.1.0",
    "browser-env": "^3.3.0",
    "css-loader": "^4.3.0",
    "cssnano": "^4.1.10",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "fast-sass-loader": "^1.5.0",
    "html-webpack-plugin": "^4.5.0",
    "jsdom": "^16.4.0",
    "mini-css-extract-plugin": "^1.0.0",
    "node-sass": "^4.14.1",
    "postcss-loader": "^4.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sinon": "^9.2.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^3.11.0"
  },
  "ava": {
    "babel": true,
    "require": [
      "@babel/register",
      "./src/test/_browser-setup.js"
    ]
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "//dependency_licenses": {
    "NOTE!": "Please keep these up to date :)",
    "@fortawesome/fontawesome-svg-core": "MIT",
    "@fortawesome/free-solid-svg-icons": "CC BY 4.0 (applied to SVG icons)",
    "@fortawesome/react-fontawesome": "MIT",
    "babel-jest": "MIT",
    "bulma": "MIT",
    "react": "MIT",
    "react-beautiful-dnd": "Apache-2.0",
    "react-dom": "MIT",
    "react-quill": "MIT",
    "react-scripts": "MIT",
    "jest": "MIT",
    "react-test-renderer": "MIT"
  }
}
